{
  "suggest.noselect": false,
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "?",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle":true,
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "json",
    "css",
    "md",
    "markdown"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
    "staticcheck": true,
    "semanticTokens": true
  },
  "cSpell.userWords": [
    "Mailgun",
    "Pearce",
    "Snowfly",
    "bpearce",
    "measureup",
    "mugadmin",
    "zonker"
  ],
  "git.enableGutters": false
}
